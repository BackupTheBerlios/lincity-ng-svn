<Panel width="384" height="270" background="images/gui/mapview/minimap_bg.png">
	<TableLayout rows="3" cols="2" >
		<rowsize row="1" fixed="30"/>
		<rowsize row="2" fixed="200"/>
		<rowsize row="3" fixed="40"/>
		<colsize col="1" fixed="174"/>
		<colsize col="2" fixed="210"/>
		<cell row="1" col="2">
			<TableLayout rows="1" cols="5">
				<cell row="1" col="1">
					<CheckButton name="SwitchMiniMap" checked="true">
						<image src="images/gui/buttons/tabbed/tabbed.png"/>
						<image-hover src="images/gui/buttons/tabbed/tabbed-hover.png"/>
						<image-clicked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<image-checked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<text-caption font-size="16">Map</text-caption>
					</CheckButton>
				</cell>
				<cell row="1" col="2">
					<CheckButton name="SwitchPBar">
						<image src="images/gui/buttons/tabbed/tabbed.png"/>
						<image-hover src="images/gui/buttons/tabbed/tabbed-hover.png"/>
						<image-clicked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<image-checked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<text-caption font-size="16">Stats</text-caption>
					</CheckButton>
				</cell>
				<cell row="1" col="3">
					<CheckButton name="SwitchGlobalMPS">
						<image src="images/gui/buttons/tabbed/tabbed.png"/>
						<image-hover src="images/gui/buttons/tabbed/tabbed-hover.png"/>
						<image-clicked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<image-checked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<text-caption font-size="16">$</text-caption>
					</CheckButton>
				</cell>
				<!--
				<cell row="1" col="4">
					<CheckButton name="SwitchMapMPS">
						<image src="images/gui/buttons/tabbed/tabbed.png"/>
						<image-hover src="images/gui/buttons/tabbed/tabbed-hover.png"/>
						<image-clicked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<image-checked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<text-caption font-size="16">?</text-caption>
					</CheckButton>
				</cell>
				-->
				<cell row="1" col="4">
					<CheckButton name="SwitchEconomyGraph">
						<image src="images/gui/buttons/tabbed/tabbed.png"/>
						<image-hover src="images/gui/buttons/tabbed/tabbed-hover.png"/>
						<image-clicked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<image-checked src="images/gui/buttons/tabbed/tabbed-clicked.png" />
						<text-caption font-size="16">Eco</text-caption>
					</CheckButton>
				</cell>
			</TableLayout>
		</cell>
		<cell row="2" col="1">
			<TableLayout rows="3" cols="3" border="false">
				<rowsize row="1" relative="1"/>
				<rowsize row="2" fixed="20"/>
				<rowsize row="3" fixed="45"/>
				<colsize col="1" relative="1"/>
				<colsize col="2" fixed="40"/>
				<colsize col="3" fixed="35"/>
				<cell row="2" col="3">
					<Button name="ZoomInButton">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-caption src="images/gui/zoom/zoomin.png"/>
						<tooltip translatable="yes">[+] Zoom in view so that less
						buildings but more details are visible</tooltip>
					</Button>
				</cell>
				<cell row="3" col="2">
					<Button name="ZoomOutButton">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-caption src="images/gui/zoom/zoomout.png"/>
						<tooltip translatable="yes">[-] Zoom out</tooltip>
					</Button>
				</cell>
			</TableLayout>
		</cell>
		<cell row="3" col="1" colspan="2">
			<TableLayout rows="1" cols="11">
				<rowsize row="1" fixed="39"/>
				<colsize col="1" fixed="39"/>
				<colsize col="2" fixed="39"/>
				<colsize col="3" fixed="39"/>
				<colsize col="4" fixed="39"/>
				<colsize col="5" relative="1"/> <!-- move content to the right side -->
				<colsize col="6" fixed="39"/>
				<colsize col="7" fixed="39"/>
				<colsize col="8" fixed="39"/>
				<colsize col="9" fixed="39"/>
				<colsize col="10" fixed="39"/>
				<colsize col="11" fixed="12"/>
				<cell row="1" col="1">
					<Button name="HelpButton">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-caption src="images/gui/info/info.png"/>
						<tooltip translatable="yes">[F1] Press this button to get help</tooltip>
					</Button>
				</cell>
				<cell row="1" col="2">
					<Button name="hideHighBuildings">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-caption src="images/gui/info/hidehigh.png"/>
						<tooltip translatable="yes">[h] Hide high buildings.
						</tooltip>
					</Button>
				</cell>
				<cell row="1" col="3">
					<Button name="StatButton">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-caption src="images/gui/info/statistics.png"/>
						<tooltip translatable="yes">Press this button	to show statistics</tooltip>
					</Button>
				</cell>
				<cell row="1" col="4">
					<Button name="mapOverlay">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-caption src="images/gui/info/mapoverlay.png"/>
						<tooltip translatable="yes">[v] Cycle through MiniMap overlay modes.
						</tooltip>
					</Button>
				</cell>
				<cell row="1" col="6">
					<CheckButton name="MapViewNormal">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
						<image-caption src="images/gui/buttonpanel/residence/residence.png"/>
						<tooltip translatable="yes">outline map</tooltip>
					</CheckButton>
				</cell>
				<cell row="1" col="7">
					<SwitchComponent name="FoodSwitch">
						<CheckButton name="MapViewFood">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/mapview/roundbuttons/round_food.png"/>
							<tooltip translatable="yes">nourishments</tooltip>
						</CheckButton>
						<CheckButton name="MapViewUB40">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/mapview/roundbuttons/round_ub40.png"/>
							<tooltip translatable="yes">unemployment</tooltip>
						</CheckButton>
					</SwitchComponent>
				</cell>
				<cell row="1" col="8">
					<SwitchComponent name="PowerSwitch">
						<CheckButton name="MapViewPower">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/buttonpanel/power/electrics.png"/>
							<tooltip translatable="yes">power supply</tooltip>
						</CheckButton>
						<CheckButton name="MapViewFire">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/buttonpanel/misc/fire.png"/>
							<tooltip translatable="yes">firedepartment cover</tooltip>
						</CheckButton>
						<CheckButton name="MapViewSport">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/buttonpanel/misc/sports.png"/>
							<tooltip translatable="yes">sport cover</tooltip>
						</CheckButton>
						<CheckButton name="MapViewHealth">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/buttonpanel/misc/health.png"/>
							<tooltip translatable="yes">medical care</tooltip>
						</CheckButton>
					</SwitchComponent>
				</cell>
				<cell row="1" col="9">
					<SwitchComponent name="TrafficSwitch">
						<CheckButton name="MapViewTraffic">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/buttonpanel/transport/road.png"/>
							<tooltip translatable="yes">traffic density</tooltip>
						</CheckButton>
						<CheckButton name="MapViewPollution">
							<image src="images/gui/buttons/round/button-round-40.png"/>
							<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
							<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
							<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
							<image-caption src="images/gui/buttonpanel/mining/trash.png"/>
							<tooltip translatable="yes">pollution density</tooltip>
						</CheckButton>
					</SwitchComponent>
				</cell>
				<cell row="1" col="10">
					<CheckButton name="MapViewCoal">
						<image src="images/gui/buttons/round/button-round-40.png"/>
						<image-hover src="images/gui/buttons/round/button-round-hover-40.png"/>
						<image-clicked src="images/gui/buttons/round/button-round-clicked-40.png"/>
						<image-checked src="images/gui/buttons/round/button-round-checked-40.png"/>
						<image-caption src="images/gui/buttonpanel/mining/coalmine.png"/>
						<tooltip translatable="yes">coal depots</tooltip>
					</CheckButton>
				</cell>
			</TableLayout>
		</cell>
		<cell row="2" col="2">
			<SwitchComponent name="MiniMapSwitch">
				<MiniMap name="MiniMap" width="200" height="200" tilesize="2"/>
				<Import src="gui/pbar.xml"/>
				<Mps name="GlobalMPS">
					<Import src="gui/mps.xml"/>
				</Mps>
				<Mps name="MapMPS">
					<Import src="gui/mps.xml"/>
				</Mps>
				<Mps name="EnvMPS">
					<Import src="gui/mps.xml"/>
				</Mps>
				<EconomyGraph name="EconomyGraph" width="200" height="200" />
			</SwitchComponent>
		</cell>
	</TableLayout>
</Panel>
