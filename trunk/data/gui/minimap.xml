<Panel width="248" height="336" background="images/gui/minimap_bg.png">
    <TableLayout rows="3" cols="2">
        <rowsize row="1" relative="10"/>
        <rowsize row="2" relative="48"/>
        <rowsize row="3" relative="288"/>
        <colsize col="1" relative="48"/>
        <colsize col="2" relative="200"/>
        <cell row="2" col="2">
            <TableLayout rows="1" cols="2">
                <cell row="1" col="1">
                    <Button name="ZoomInButton">
                        <image src="images/gui/buttonpanel/background/button.png"/>
                        <image-hover src="images/gui/buttonpanel/background/button-hover.png"/>
                        <image-clicked src="images/gui/buttonpanel/background/button-clicked.png"/>
			<image-caption src="images/gui/mapview/buttons/zoomin.png"/>
                    </Button>
                </cell>
                <cell row="1" col="2">
                    <Button name="ZoomOutButton">
                        <image src="images/gui/buttonpanel/background/button.png"/>
                        <image-hover src="images/gui/buttonpanel/background/button-hover.png"/>
                        <image-clicked src="images/gui/buttonpanel/background/button-clicked.png"/>
			<image-caption src="images/gui/mapview/buttons/zoomout.png"/>
                    </Button>
                </cell>
            </TableLayout>
        </cell>
        <cell row="3" col="1">
            <TableLayout rows="9" cols="1">
                <cell row="1" col="1">
                    <Button name="MapViewNormal">
			<image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
                        <image-caption src="images/gui/mapview/roundbuttons/round_resident.png"/>
<!--                        <image src="images/gui/mapview_buildings.png"/>-->
                    </Button>
                </cell>
                <cell row="2" col="1">
                    <Button name="MapViewUB40">
			<image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
			<image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
			<image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
                        <image-caption src="images/gui/mapview/roundbuttons/round_ub40.png"/>
<!--                        <image src="images/gui/mapview_transparent.png"/>-->
                    </Button>
                </cell>
                <cell row="3" col="1">
                    <Button name="MapViewPollution">
			<image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
                         <image-caption src="images/gui/mapview/roundbuttons/round_pollution.png"/>
<!--                        <image src="images/gui/mapview_polution.png"/>-->
                    </Button>
                </cell>
                <cell row="4" col="1">
                    <Button name="MapViewFood">
			    <image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
			<image-caption src="images/gui/mapview/roundbuttons/round_food.png"/>
<!--                        <image src="images/gui/mapview_noidea.png"/>-->
                    </Button>
                </cell>
                <cell row="5" col="1">
                    <Button name="MapViewPower">
			<image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
			<image-caption src="images/gui/mapview/roundbuttons/round_power.png"/>
<!--                        <image src="images/gui/mapview_sport.png"/>-->
                    </Button>
                </cell>
                <cell row="6" col="1">
                    <Button name="MapViewFire">
                        <image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
			<image-caption src="images/gui/mapview/roundbuttons/round_fire.png"/>
<!--                        <image src="images/gui/mapview_fire.png"/>-->
                    </Button>
                </cell>
                <cell row="7" col="1">
                    <Button name="MapViewSport">
                        <image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
			<image-caption src="images/gui/mapview/roundbuttons/round_soccer.png"/>
<!--                        <image src="images/gui/mapview_health.png"/>-->
                    </Button>
                </cell>
                <cell row="8" col="1">
                    <Button name="MapViewHealth">
                        <image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
			<image-caption src="images/gui/mapview/roundbuttons/round_health.png"/>
<!--                        <image src="images/gui/mapview_health.png"/>-->
                    </Button>
                </cell>
                <cell row="9" col="1">
                    <Button name="MapViewCoal">
                        <image src="images/gui/mapview/roundbuttons/round_button_bg0.png"/>
                        <image-clicked src="images/gui/mapview/roundbuttons/round_button_bg2.png"/>
                        <image-hover src="images/gui/mapview/roundbuttons/round_button_bg1.png"/>
			<image-caption src="images/gui/mapview/roundbuttons/round_coal.png"/>
<!--                        <image src="images/gui/mapview_health.png"/>-->
                    </Button>
                </cell>
            </TableLayout>
        </cell>
        <cell row="3" col="2">
            <TableLayout rows="2" cols="1">
                <rowsize row="1" relative=".8"/>
                <rowsize row="2" relative=".2"/>
                <cell row="1" col="1">
                    <MiniMap width="200" height="200" tilesize="2"/>
                </cell>
                <cell row="2" col="1">
                    <TableLayout rows="1" cols="4">
                        <cell row="1" col="1">
                            <Button name="SpeedPauseButton">
                                <image src="images/gui/buttonpanel/background/button.png"/>
                                <image-hover src="images/gui/buttonpanel/background/button-hover.png"/>
                                <image-clicked src="images/gui/buttonpanel/background/button-clicked.png"/>
                                <image-caption src="images/gui/speed/pause.png"/>
                            </Button>
                        </cell>
                        <cell row="1" col="2">
                            <Button name="SpeedSlowButton">
                                <image src="images/gui/buttonpanel/background/button.png"/>
                                <image-hover src="images/gui/buttonpanel/background/button-hover.png"/>
                                <image-clicked src="images/gui/buttonpanel/background/button-clicked.png"/>
                                <image-caption src="images/gui/speed/normal.png"/>
                            </Button>
                        </cell>
                        <cell row="1" col="3">
                            <Button name="SpeedNormalButton">
                                <image src="images/gui/buttonpanel/background/button.png"/>
                                <image-hover src="images/gui/buttonpanel/background/button-hover.png"/>
                                <image-clicked src="images/gui/buttonpanel/background/button-clicked.png"/>
                                <image-caption src="images/gui/speed/fast.png"/>
                            </Button>
                        </cell>
                        <cell row="1" col="4">
                            <Button name="SpeedFastButton">
                                <image src="images/gui/buttonpanel/background/button.png"/>
                                <image-hover src="images/gui/buttonpanel/background/button-hover.png"/>
                                <image-clicked src="images/gui/buttonpanel/background/button-clicked.png"/>
                                <image-caption src="images/gui/speed/faster.png"/>
                            </Button>
                        </cell>
                    </TableLayout>
                </cell>
            </TableLayout>
        </cell>
    </TableLayout>
</Panel>

